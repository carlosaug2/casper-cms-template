< feed  xmlns = " http://www.w3.org/2005/Atom " >
  < title > {{with .Title}} {{.}} em {{end}} {{.Site.Title}} </ title >
  < link  rel = " self "  href = " {{.Permalink}} " />
  < atualizado > {{.Date.Format "2006-01-02T15: 04: 05-0700" | safeHTML}} </ atualizado >
  < autor >
    < nome > SEU NOME AQUI </ name >
    < email > SEU ENDEREÇO ​​DE EMAIL </ email >
    < uri > URI DEFINITIVO DO SEU SITE </ uri >
  </ autor >
  < id > {{.Permalink}} </ id >
  {{range first 15 .Data.Pages}}
  < entrada >
    < title > {{.Title}} </ title >
    < link  rel = " alternate "  href = " {{.Permalink}} " />
    < id > {{.Permalink}} </ id >
    < publicado > {{.Date.Format "2006-01-02T15: 04: 05-0700" | safeHTML}} </ publicado >
    < atualizado > {{.Lastmod.Format "2006-01-02T15: 04: 05-0700" | safeHTML}} </ atualizado >
    < resumo > {{.Resumo | html}} </ resumo >
  </ entrada >
  {{ fim }}
</ feed >
